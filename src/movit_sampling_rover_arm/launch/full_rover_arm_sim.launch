<launch>
<!-- Launch Your robot arms launch file which loads the robot in Gazebo and spawns the
controllers -->
<include file = "$(find sampling_rover_with_arm_urdf)/launch/rover_arm_urdf_rocks.launch" />
<!-- Launch Moveit Move Group Node -->
<include file = "$(find movit_sampling_rover_arm)/launch/move_group.launch" />
<!-- Run Rviz and load the default configuration to see the state of the move_group node -->
<arg name="use_rviz" default="true" />
<include file="$(find movit_sampling_rover_arm)/launch/moveit_rviz.launch" if="$(arg use_rviz)">
<arg name="rviz_config" value="$(find movit_sampling_rover_arm)/launch/moveit.rviz"/>
</include>
</launch>